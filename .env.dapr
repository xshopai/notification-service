# Service Configuration
SERVICE_NAME=notification-service
VERSION=1.0.0
PORT=8011
NODE_ENV=development

# Service Invocation Mode
SERVICE_INVOCATION_MODE=dapr

# Dapr Configuration
DAPR_HTTP_PORT=3511
DAPR_GRPC_PORT=50011

# Messaging - use Dapr pubsub
MESSAGING_PROVIDER=dapr

# Logging
LOG_LEVEL=debug
LOG_FORMAT=console
LOG_TO_CONSOLE=true
LOG_TO_FILE=false
LOG_FILE_PATH=./logs/notification-service.log
